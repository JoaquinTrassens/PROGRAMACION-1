#include <stdio.h>
//EJERCICIO 2.1
int main()
{
    
    int numero; 
    int cont = 1; 
    int cont2 = 0; 
    
    printf("Ingrese numero: "); 
    scanf("%d", &numero); 
    for(int i = 0; i < numero; i++){
    
        if(numero % cont == 0){
            
            cont2++;
             
            cont++; 
            
        }

    }
    
    printf("\n La cantidad de numeros divisores son: %d", cont2);



    return 0;
}
